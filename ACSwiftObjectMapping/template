import Foundation

public class ClassName {

	properties

    propertyGetter

    public class func modelsFromDictionaryArray(array:NSArray) -> [ClassName]
    {
        var models:[ClassName] = []
        for item in array
        {
            models.append(ClassName(dictionary: item as! NSDictionary)!)
        }
        return models
    }

	required public init?(dictionary: NSDictionary) {

		initInside
	}

	public func dictionaryRepresentation() -> NSDictionary {

		let dictionary = NSMutableDictionary()

		dictionaryInside

		return dictionary
	}

}
